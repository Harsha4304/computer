
# create a data frame
dataframe1 <- data.frame (
  Name = c("Juan", "Alcaraz"),
  Age = c(22, 15)
)

# create another data frame
dataframe2 <- data.frame (
  Name = c("Yiruma", "Bach", "Ludovico"),
  Age = c(46, 89, 72)
)

# make a list of dataframe1 and dataframe2
print(list(dataframe1, dataframe2))
